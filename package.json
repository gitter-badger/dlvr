{
  "name": "dlvr",
  "version": "3.2.1",
  "description": "DeLiVeR - Command Line Tool to automate releasing processes on GitHub/GitLab/npm",
  "bin": {
    "dlvr": "./src/index.js"
  },
  "files": [
    "src/",
    "package.json"
  ],
  "scripts": {
    "test:coverage": "istanbul cover --include-all-sources --root ./src jasmine-node ./tests",
    "codacyreport": "cat ./coverage/lcov.info | codacy-coverage",
    "test:watch": "jasmine-node --verbose --autotest --color --growl --test-dir ./tests --watch ./src",
    "test": "npm run lint && npm run jasmine",
    "ci": "depcheck && npm run lint && npm run test:coverage && npm run codacyreport",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "release": "npm test && npm run build && dlvr",
    "jasmine": "jasmine-node --test-dir ./tests",
    "pkg:upgrade": "ncu -a -u && rm -rf node_modules && npm i",
    "pkg:test": "ncu && nsp check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freakzero/dlvr"
  },
  "keywords": [
    "npm",
    "release",
    "changelog"
  ],
  "author": "FreaKzero",
  "license": "MIT",
  "dependencies": {
    "argparse": "1.0.9",
    "chalk": "2.3.0",
    "dotenv": "^4.0.0",
    "irc": "0.5.2",
    "is-my-json-valid": "2.16.1",
    "mime": "2.0.3",
    "node-async-loop": "1.2.2",
    "node-notify": "^1.0.0",
    "object-filter": "1.0.2",
    "octonode": "0.9.1",
    "opn": "^5.1.0",
    "ora": "1.3.0",
    "prompt": "1.0.0",
    "request": "^2.83.0",
    "semver": "5.4.1",
    "simple-git": "1.80.1",
    "zip-local": "0.3.4"
  },
  "devDependencies": {
    "eslint": "4.10.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-import-order": "2.1.4",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "jasmine-node": "1.14.5",
    "prettier": "^1.7.4",
    "proxyquire": "1.8.0",
    "npm-check-updates": "*",
    "nsp": "*"
  }
}
